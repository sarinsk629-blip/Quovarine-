# Quovarine Environment Variables Template
# Copy this file to .env and fill in your values

# OpenAI API Key for AI functionality
OPENAI_API_KEY=your_openai_api_key_here

# Vercel Deployment Credentials
# Get these from: https://vercel.com/account/tokens
VERCEL_TOKEN=your_vercel_token_here

# Get these from your Vercel project settings
# Project Settings -> General -> Project ID
VERCEL_PROJECT_ID=your_vercel_project_id_here

# Get this from your Vercel account settings
# Account Settings -> General -> Team/User ID
VERCEL_ORG_ID=your_vercel_org_id_here

# Deployment URL for health monitoring
# This will be your production URL (e.g., https://quovarine.vercel.app)
DEPLOYMENT_URL=https://your-deployment-url.vercel.app

# Node environment (development, production, test)
NODE_ENV=development
